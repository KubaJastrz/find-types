    "playwright-msw": "^3.0.1",
    "postcss": "^8.4.38",
    "remix-flat-routes": "^0.6.5",
    "tailwindcss": "^3.4.3",
    "type-fest": "^4.18.3",
    "typescript": "~5.4.5",
    "vite": "^5.2.12",
    "vite-tsconfig-paths": "^4.3.2"
