{
  "name": "find-types",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "cross-env NODE_ENV=production vite build",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "tsc && eslint . --ext .js,.ts,.tsx",
    "deploy": "now"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,tsx,json,md,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@now/node": "^1.7.4",
