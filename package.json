    "clsx": "^1.1.1",
    "downshift": "^6.0.3",
    "focus-visible": "^5.1.0",
    "history": "^5.0.0",
    "ky": "^0.23.0",
    "react-helmet-async": "^1.0.6",
    "react-query": "^2.5.13",
    "react-router-dom": "^6.0.0-beta.0",
    "tailwindcss": "^1.6.2"
  },
  "devDependencies": {
