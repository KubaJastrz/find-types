    "@tanstack/react-query": "^5.40.0",
    "@vercel/remix": "^2.9.2",
    "dompurify": "^3.1.4",
    "downshift": "7.2.1",
    "hosted-git-info": "^7.0.2",
    "isbot": "^5.1.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remix-utils": "^7.6.0",
    "tailwind-merge": "^2.3.0"
