{
  "name": "find-types",
  "private": true,
  "version": "2.0.0",
  "scripts": {
    "start": "now dev",
    "now-dev": "gatsby develop",
    "deploy": "now",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "validate": "npm run lint && npm run test",
    "test": "jest",
    "lint": "tsc && eslint . --ext .js,.ts,.tsx"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,tsx,json,md}": [
      "prettier --write"
    ]
  },
